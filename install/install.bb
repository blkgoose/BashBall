#!/usr/bin/env bash
set -e;declare -a apps=("tar" "base64");for app in ${apps[@]}; do hash $app 2>/dev/null || { echo "$app is not installed on this system"; exit 1; }; done;export _localdir="$(pwd)";export _tmpdir="/tmp/bashball-$$";export _name="$(basename $0)";source() {    lib="$1";    cd "$_tmpdir";    . "$lib";    cd "$_localdir";};export -f source;trap "rm -rf $_tmpdir" EXIT;mkdir -p $_tmpdir;printf $'' | base64 -d | tar xz -C $_tmpdir --warning=no-timestamp;bash -c "$(cat <(echo 'cd $_localdir') $_tmpdir/main.sh)" $_name ${@};